{
    "manifest_version": 2,
    "name": "Google Meet 출석 체크",
    "description": "Google Meet 전용 출석 체크입니다.",
    "version": "1.0",
    "icons": {
        "16": "icon/icon.png",
        "48": "icon/icon.png",
        "128": "icon/icon.png"
    },
    "browser_action": {
        "default_icon": "icon/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://meet.google.com/*"
    ]
}